{
  "name": "researchhub",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "sentry": "node ./scripts/sentry.js",
    "cypress:open": "cypress open --project $PWD/tests",
    "test": "cypress run --project $PWD/tests",
    "build:prod": "docker build --build-arg SENTRY_RELEASE=production-$(git rev-parse --short HEAD) -t researchhub-web .",
    "build:staging": "docker build --build-arg SENTRY_RELEASE=staging-$(git rev-parse --short HEAD) --build-arg REACT_APP_ENV=staging -t researchhub-web-staging .",
    "deploy": "./scripts/deploy/production.sh deploy",
    "staging": "./scripts/deploy/staging.sh deploy",
    "run:staging": "docker run",
    "start:prod": "docker run -it -p 3010:3000 researchhub-web",
    "start:staging": "docker run -it -p 3010:3000 researchhub-web-staging",
    "sitemap": "node scripts/createSitemap.js",
    "sitemap-index": "node scripts/createSitemapIndex.js"
  },
  "dependencies": {
    "@quantfive/js-web-config": "latest",
    "@quantfive/react-burger-menu": "^2.6.19",
    "@quantfive/react-image-upload-grid": "1.1.3",
    "@sentry/browser": "^5.9.1",
    "@sentry/cli": "^1.49.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "aphrodite": "^2.4.0",
    "axios": "^0.19.2",
    "diff": "^4.0.1",
    "fslightbox-react": "^1.4.9",
    "immutability-helper": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-unfetch": "^3.0.0",
    "javascript-time-ago": "^2.0.7",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.2",
    "katex": "^0.11.1",
    "moment": "^2.24.0",
    "next": "^9.0.6",
    "next-compose-plugins": "^2.2.0",
    "next-redux-wrapper": "^4.0.0",
    "next-transpile-modules": "^2.3.1",
    "nuka-carousel": "^4.6.6",
    "numeral": "^2.0.6",
    "progress": "^2.0.3",
    "q5-js-config": "^1.4.1",
    "react": "^16.10.1",
    "react-alert": "^6.0.0",
    "react-alert-template-basic": "^1.0.0",
    "react-autosize-textarea": "^7.0.0",
    "react-avatar": "^3.7.0",
    "react-avatar-edit": "^0.8.3",
    "react-burger-menu": "^2.6.11",
    "react-collapsible": "^2.8.0",
    "react-confetti": "4.0.1",
    "react-device-detect": "^1.9.10",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.10.1",
    "react-dropzone": "^10.1.9",
