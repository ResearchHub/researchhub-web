  },
  "devDependencies": {
    "cypress": "^3.4.1",
    "husky": "3.0.8",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn run test"
    }
  }
}
