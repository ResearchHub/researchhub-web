    "react-google-login": "^5.0.5",
    "react-hotjar": "^2.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-joyride": "^2.1.1",
    "react-modal": "^3.10.1",
    "react-modal-image": "^2.5.0",
    "react-pdf": "^4.1.0",
