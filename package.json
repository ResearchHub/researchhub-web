{
  "name": "researchhub",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "sentry": "node ./scripts/sentry.js",
    "cypress:open": "cypress open --project $PWD/tests",
    "test": "cypress run --project $PWD/tests",
    "build:prod": "docker build --build-arg SENTRY_RELEASE=production-$(git rev-parse --short HEAD) -t researchhub-web .",
    "build:staging": "docker build --build-arg SENTRY_RELEASE=staging-$(git rev-parse --short HEAD) --build-arg REACT_APP_ENV=staging -t researchhub-web-staging .",
    "deploy": "./scripts/deploy/production.sh deploy",
    "staging": "./scripts/deploy/staging.sh deploy",
    "run:staging": "docker run",
    "start:prod": "docker run -it -p 3010:3000 researchhub-web",
    "start:staging": "docker run -it -p 3010:3000 researchhub-web-staging",
    "sitemap": "node scripts/createSitemap.js",
    "sitemap-index": "node scripts/createSitemapIndex.js",
    "eb:deploy": "./scripts/deploy/eb-deploy.sh"
  },
  "dependencies": {
